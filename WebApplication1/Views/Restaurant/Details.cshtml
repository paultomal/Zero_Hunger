@model WebApplication1.EF.Restaurant_Table

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    
 
    <dl class="dl-horizontal">
        <dt>
            Restaurant Name:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Restaurant_Name)
        </dd>

        <dt>
            Address:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Address)
        </dd>

        <dt>
           Contact:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Contact)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Restaurant_ID }) |   
    <a class="btn btn-dark" href="/Restaurant/Index">Back</a>
</p>
